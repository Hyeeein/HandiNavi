    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "df1 = pd.read_csv('구별 장애인 수.csv', encoding='utf-8-sig')\n",
    "\n",
    "df1 = df1.loc[3:,['자치구', '합계']]\n",
    "\n",
    "for i,data in enumerate(df1['합계']):\n",
    "  df1['합계'][i] = data.replace(',','')\n",
    " \n",
    "df1.sort_values(by=['합계', '자치구'])"
   ]
